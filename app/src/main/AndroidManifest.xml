<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pwittchen.com.swipedetector">

  <application
      android:allowBackup="true"
      android:icon="@drawable/ic_launcher"
      android:label="@string/app_name"
      android:theme="@style/AppTheme">
    <activity
        android:label="@string/listener"
        android:name="com.github.pwittchen.swipedetector.app.SwipeDetectorActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity
        android:label="@string/rx"
        android:name="com.github.pwittchen.swipedetector.app.SwipeDetectorReactiveActivity"/>
  </application>

</manifest>
